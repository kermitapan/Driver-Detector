# Codemagic Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file with real credentials

# Android Signing
ANDROID_KEYSTORE=<base64_encoded_keystore_file>
ANDROID_KEYSTORE_PASSWORD=your_keystore_password
ANDROID_KEY_PASSWORD=your_key_password
ANDROID_KEY_ALIAS=your_key_alias
PACKAGE_NAME=com.driverdetector.app

# iOS Signing
BUNDLE_ID=com.driverdetector.app
APP_STORE_CONNECT_PRIVATE_KEY=<your_private_key>
APP_STORE_CONNECT_KEY_IDENTIFIER=<your_key_id>
APP_STORE_CONNECT_ISSUER_ID=<your_issuer_id>
CERTIFICATE_PRIVATE_KEY=<your_certificate_private_key>

# Google Play Publishing
GCLOUD_SERVICE_ACCOUNT_CREDENTIALS=<your_service_account_json>

# Notification
EMAIL_RECIPIENTS=user@example.com

# Feature Flags (Optional)
ENABLE_DRIVER_DETECTION=true
ENABLE_CAR_MOVEMENT=true
ENABLE_CRASH_DETECTION=true

# API Keys (if needed for features)
MAPS_API_KEY=your_maps_api_key
ML_MODEL_API_KEY=your_ml_api_key
